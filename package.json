{
  "name": "functional-oriented-programming-ts",
  "version": "0.0.1",
  "description": "Function Oriented Programming (FOP) in Typescript",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p .",
    "prettier": "prettier --list-different \"./{src}/**/*.ts\"",
    "fix-prettier": "prettier --write \"./{src}/**/*.ts\"",
    "clean": "rimraf ./dist",
    "prerelease": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json"
  },
  "author": "David Shekunts <ditreyw@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dionid/functional-oriented-programming-ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/prettier": "^2.4.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.4.4"
  },
  "bugs": {
    "url": "https://github.com/Dionid/functional-oriented-programming-ts/issues"
  },
  "homepage": "https://github.com/Dionid/functional-oriented-programming-ts",
  "tags": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "keywords": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ]
}
